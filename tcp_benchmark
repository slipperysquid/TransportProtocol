import socket
import time

sock = socket.socket(socket.AF_INET,socket.SOCK_STREAM)

conn = sock.connect(('185.77.96.12',5001))


with open("stuff.txt",encoding="utf8") as f:
    start = time.time()
    sock.send(f.read().encode())
    end = time.time()

print('TCP finished in {} seconds'.format(end - start))